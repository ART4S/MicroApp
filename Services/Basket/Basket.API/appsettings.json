{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },

  "AllowedHosts": "*",

  //"Kestrel": {
  //  "EndpointDefaults": {
  //    "Protocols": "Http2"
  //  }
  //},

  "BackgroundTasks": {
    "PollingIntervalSec": 1,
    "BasketExpirationDays": 7,
    "Schedule": {
      "DeleteExpiredBaskets": "*/5 * * * * *"
    }
  }
}
