{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },

  "AllowedHosts": "*",

  "BackgroundTasks": {
    "PollingIntervalSec": 5,
    "IntegrationEventSchedule": "*/5 * * * * *"
  }
}
